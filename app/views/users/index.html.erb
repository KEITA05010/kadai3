<%= render 'user_info', user: @user %>

<div class="col-md-8 offset-md-1">

<h2>Users</h2>
<table class="table table-hover table-inverse">
  
<thead>
   <tr>
      
      <th>image</th>
      <th>name</th>
      
   </tr>
</thead>
  
 <tbody>
      <%@users.each do |user| %>


        <tr>
            <td><%=image_tag(user.get_profile_image)%></td>
            <td><%=user.name%></td>
            <td><%=link_to "show",user_path(user) %></td>
            
        </tr>
        
  
    
   
  <%end%>
      
 </tbody>
 </table>
    
</div>
